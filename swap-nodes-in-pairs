# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def swapPairs(self, head):
        if head==None or head.next==None:
            return head
        
        temp=head.next #storing B in temp
        head.next=self.swapPairs(temp.next) #storing the swapped part of 3-4 into A's next
        temp.next=head

        return temp
